{
	"compilerOptions": {
		"moduleResolution": "node",
		"module": "CommonJS",
		"rootDir": "./",
		"types": ["node", "wdio-vscode-service", "mocha", "@wdio/globals/types"],
		"target": "es2022",
		"allowJs": true,
		"esModuleInterop": true,
		"skipLibCheck": true,
		"resolveJsonModule": true
	},
	"include": ["**/*.ts", "**/*.js"],
	"exclude": ["node_modules"]
}
